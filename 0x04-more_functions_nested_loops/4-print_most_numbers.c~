#include "main.h"

/**
 * print_most_numbers - prints all single digit number
 * except 2 and 4
 * Return: Always 0 (Success);
 */
void print_most_numbers(void)
{
	int i = 48;

	while (i <= 57)
	{
		if (i == 50)
		{
			continue;
		}
		else if (i == 52)
		{
			continue;
		}
		_putchar(i);
		++i;
	}
	_putchar('\n');
}
